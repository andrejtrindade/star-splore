pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
#include ../scraper/results/featured_carts_extra.lua

star_splore_file="../star_splore.p8"

addr=0x0000
function temp_add(v)
	poke(addr+0x8000,v)
	addr += 1
end

can_add=true
for num=1,#data do
	local cart=data[num]
	if (addr+#cart>0x42fd) can_add=false
	if can_add then
		for i=1,#cart do 
			temp_add(ord(cart[i])) 
		end
		temp_add(0)
	else
		stop("error: cant add cart #"..num)
	end
end
temp_add(0xa)
print("rom usage: "..(addr/0x4300*100).."%\n")
cstore(0x0000, 0x8000, addr, star_splore_file)
stop(tostr(#data).." carts written to star splore")

